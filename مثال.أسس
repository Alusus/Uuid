اشمل "معرف_عالمي.أسس"؛
اشمل "مـتم/طـرفية"؛
اشمل "مـتم/نـص"؛
اشمل "مـتم/مـصفوفة"؛

دالة  اختبار{

  استخدم مـعرف_عالمي;
  استخدم مـتم.طـرفية;
  
   عرف  ع1: مـعرف;
   عرف  ع2: مـعرف;

   انشاء (ع1);
  
  عرف  نص: مصفوفة[محرف, 100];
 
  اطبع("Enter Uuid: ");
  عرف  مدخل: مصفوفة[محرف, 100];
  أدخل_محارف(مدخل~مؤشر, 100);
 
  تحليل (مدخل~مؤشر, ع1);

   اطبع("Enter other Uuid: ");
  عرف  نص1: مصفوفة[محرف, 100];
  أدخل_محارف(نص1~مؤشر, 100);
  
  تحليل (مدخل~مؤشر, ع2);
  
  غير_محلل (ع1, نص~مؤشر);
  اطبع("%s \n",  نص~مؤشر);
  
   غير_محلل (ع2, نص~مؤشر);
   اطبع("%s \n",  نص~مؤشر);
  
  إذا قارن (ع1, ع2) == 0
  اطبع("is equal")
  
  وإلا 
  اطبع("is not equal");
   

}

اختبار();




